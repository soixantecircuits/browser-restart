{
  "name": "browserrestart",
  "productName": "Browserrestart",
  "version": "1.0.0",
  "description": "browserrestart by Art2B",
  "license": "MIT",
  "repository": "Art2B/browserrestart",
  "author": {
    "name": "Arthur Battut",
    "email": "arthur@soixantecircuits.fr",
    "url": "https://github.com/soixantecircuits/browser-restart"
  },
  "electronVersion": "0.35.0",
  "scripts": {
    "start": "export NODE_ENV='dev'; electron .",
    "build": "export NODE_ENV='production'; electron-packager . $npm_package_productName --out=bin --ignore='^/bin$' --prune --asar --all --version=$npm_package_electronVersion --overwrite",
    "build-osx": "export NODE_ENV='production'; electron-packager . $npm_package_productName --out=bin --platform=darwin --arch=x64 --ignore='^/bin$' --prune --version=$npm_package_electronVersion --overwrite",
    "build-linux": "export NODE_ENV='production'; electron-packager . $npm_package_productName --out=bin --platform=linux --arch=x64 --ignore='^/bin$' --prune --version=$npm_package_electronVersion --overwrite",
    "build-win": "export NODE_ENV='production'; electron-packager . $npm_package_productName --out=bin --platform=win --arch=x64 --ignore='^/bin$' --prune --version=$npm_package_electronVersion --overwrite"
  },
  "files": [
    "index.js",
    "app/"
  ],
  "dependencies": {
    "crossroads": "^0.12.2",
    "electron-debug": "^0.5.0",
    "hasher": "^1.2.0",
    "signals": "^1.0.0",
    "body-parser": "^1.14.2",
    "browser-launcher2": "^0.4.6",
    "express": "^4.13.3",
    "jade": "^1.11.0",
    "lokijs": "^1.3.10",
    "socket.io": "^1.3.7"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "css-loader": "^0.23.0",
    "electron-packager": "^5.0.0",
    "electron-prebuilt": "^0.35.0",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.0",
    "gulp-connect": "^2.2.0",
    "html-loader": "^0.4.0",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9"
  }
}
